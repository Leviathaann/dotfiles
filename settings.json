{
  "editor.fontFamily": "'FiraCode Nerd Font', 'Courier New', monospace",
  "editor.fontLigatures": true,
  "redhat.telemetry.enabled": false,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "git.autofetch": true,
  "[javascript]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "editor.formatOnSave": true,
  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "workbench.startupEditor": "none",
  "cmake.showOptionsMovedNotification": false,
  "editor.fontSize": 15,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDelete": false,
  "dev.containers.executeInWSL": true,
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "workbench.sideBar.location": "right",
  "files.autoSave": "onFocusChange",
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.colorTheme": "Vira Graphene",
  "material-icon-theme.hidesExplorerArrows": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "[Vira*]": {
      "toolbar.activeBackground": "#E9A58126",
      "button.background": "#E9A581",
      "button.hoverBackground": "#E9A581cc",
      "extensionButton.separator": "#E9A58133",
      "extensionButton.background": "#E9A58114",
      "extensionButton.foreground": "#E9A581",
      "extensionButton.hoverBackground": "#E9A58133",
      "extensionButton.prominentForeground": "#E9A581",
      "extensionButton.prominentBackground": "#E9A58114",
      "extensionButton.prominentHoverBackground": "#E9A58133",
      "activityBarBadge.background": "#E9A581",
      "activityBar.activeBorder": "#E9A581",
      "activityBarTop.activeBorder": "#E9A581",
      "list.inactiveSelectionIconForeground": "#E9A581",
      "list.activeSelectionForeground": "#E9A581",
      "list.inactiveSelectionForeground": "#E9A581",
      "list.highlightForeground": "#E9A581",
      "sash.hoverBorder": "#E9A58180",
      "list.activeSelectionIconForeground": "#E9A581",
      "scrollbarSlider.activeBackground": "#E9A58180",
      "editorSuggestWidget.highlightForeground": "#E9A581",
      "textLink.foreground": "#E9A581",
      "progressBar.background": "#E9A581",
      "pickerGroup.foreground": "#E9A581",
      "tab.activeBorder": "#E9A581",
      "notificationLink.foreground": "#E9A581",
      "editorWidget.resizeBorder": "#E9A581",
      "editorWidget.border": "#E9A581",
      "settings.modifiedItemIndicator": "#E9A581",
      "panelTitle.activeBorder": "#E9A581",
      "breadcrumb.activeSelectionForeground": "#E9A581",
      "menu.selectionForeground": "#E9A581",
      "menubar.selectionForeground": "#E9A581",
      "editor.findMatchBorder": "#E9A581",
      "selection.background": "#E9A58140",
      "statusBarItem.remoteBackground": "#E9A58114",
      "statusBarItem.remoteHoverBackground": "#E9A581",
      "statusBarItem.remoteForeground": "#E9A581",
      "notebook.inactiveFocusedCellBorder": "#E9A58180",
      "commandCenter.activeBorder": "#E9A58180",
      "chat.slashCommandForeground": "#E9A581",
      "chat.avatarForeground": "#E9A581",
      "activityBarBadge.foreground": "#000000",
      "button.foreground": "#000000",
      "statusBarItem.remoteHoverForeground": "#000000"
    }
  },
  "workbench.iconTheme": "vira-icons-teal",
  "viraTheme.accent": "Vira",
  "workbench.activityBar.location": "hidden",
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "go": false
  }
}
